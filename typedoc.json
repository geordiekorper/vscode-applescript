{
	"entryPoints": [
		"src/index.ts",
		"src/outline.ts",
		"src/osa.ts",
		"src/processes.ts",
		"src/task.ts",
		"src/util.ts",
		"types/index.d.ts"
	],
	"out": "docs/typedoc-md",
	"includeVersion": true,
	"router": "module",
	"sort": ["source-order"],
	"excludePrivate": true,
	"excludeProtected": true,
	"excludeExternals": true,
	"exclude": ["test/**", "**/test/**", "lib/**"],
	"skipErrorChecking": true,
	"tsconfig": "tsconfig.json",
	"plugin": ["typedoc-plugin-markdown"],
	"hideGenerator": false,
	"readme": "none"
}
